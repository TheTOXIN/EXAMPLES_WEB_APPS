<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>HelloDisplayName</display-name>

  <welcom-file-list>
  	<welcom-file>index.html</welcom-file>
  </welcom-file-list>

  <servlet> 
    <servlet-name>HelloServlet</servlet-name> 
      <servlet-class>com.toxin.hello.HelloServlet</servlet-class> 
  </servlet> 

  <servlet-mapping> 
    <servlet-name>HelloServlet</servlet-name> 
    <url-pattern>/HelloServlet</url-pattern> 
  </servlet-mapping> 

  <servlet> 
    <servlet-name>FormServlet</servlet-name> 
    <servlet-class>com.toxin.hello.FormServlet</servlet-class> 
  </servlet> 

  <servlet-mapping> 
    <servlet-name>FormServlet</servlet-name> 
    <url-pattern>/FormServlet</url-pattern> 
  </servlet-mapping>

</web-app>
